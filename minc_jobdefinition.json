{
    "jobDefinitionName": "run-minc",
    "type": "container",
    "containerProperties": {
        "image": "168045869720.dkr.ecr.us-east-1.amazonaws.com/maget_fetchrun",
        "vcpus": 16,
        "memory": 27000,
        "command": [
        	"MINC_JobScript_seq.sh"
        ],
        "volumes": [
            {
                "host": {
                    "sourcePath": "/maget"
                },
                "name": "efs"
            }
        ],
        "environment": [
            {
                "name": "BATCH_FILE_S3_URL",
                "value": "s3://scripts-abcdgruen/MINC_JobScript_seq.sh"
            },
            {
                "name": "BATCH_FILE_TYPE",
                "value": "script"
            }
        ],
        "mountPoints": [
            {
                "containerPath": "/maget",
                "sourceVolume": "efs"
            }
        ],
        "jobRoleArn": "arn:aws:iam::168045869720:role/IAMconfig-Ec2S3FullAccessRole-1RRQ57X5ZHDPV",
        "readonlyRootFilesystem": false,
        "privileged": true
    },
    "retryStrategy": {
		"attempts": 2,
		"evaluateOnExit": [
			{
			  "onStatusReason": "Host EC2*",
			  "action": "RETRY"
			},
			{
      		  "onReason": "*",
      		  "onExitCode": "*",
			  "action": "EXIT"
			}
  		]
    }
}